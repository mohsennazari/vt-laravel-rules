{
    "name": "monaz/vt-laravel-rules",
    "description": "Set of useful validation rules using VirusTotal API for Laravel",
    "type": "library",
    "keywords": ["virus","virustotal", "laravel", "laravel-validation-rules", "security"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohsen Nazari",
            "email": "mohsennazari90@gmail.com",
            "homepage": "http://mohsen.codes",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "monaz/vt-php-api3": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "orchestra/testbench": "6.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Monaz\\LaravelVirusTotal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Monaz\\LaravelVirusTotal\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Monaz\\LaravelVirusTotal\\LaravelVirusTotalServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
